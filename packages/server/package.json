{
  "name": "@autolink/server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "db:generate": "prisma generate",
    "dev": "dotenv -e ../../.env.dev -- tsx watch src/main.ts",
    "start": "dotenv -e ../../.env.dev -- tsx src/main.ts",
    "start:prod": "dotenv -e ../../.env.prod -- node dist/main.js",
    "lint": "biome check src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@autolink/shared": "workspace:*",
    "@hono/node-server": "^1.19.9",
    "@hono/zod-validator": "^0.5.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "bull": "^4.16.5",
    "fp-ts": "^2.16.11",
    "hono": "^4.7.4",
    "ioredis": "^5.5.0",
    "pg": "^8.16.0",
    "pino": "^10.3.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@autolink/tsconfig": "workspace:*",
    "@testcontainers/postgresql": "^10.0.0",
    "@types/node": "^22.0.0",
    "@types/pg": "^8.11.0",
    "@types/pino": "^7.0.5",
    "dotenv-cli": "^11.0.0",
    "pino-pretty": "^13.1.3",
    "prisma": "^7.2.0",
    "testcontainers": "^10.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.0"
  }
}
